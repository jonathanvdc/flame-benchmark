using System;

public module PointTest
{
    public static void Main(int InputSize)
    {
        int iterations = InputSize;

        Point a = new Point(1, 1), b = new Point(1, 1);
        for (int i = 0; i < iterations; i++)
            a = Ops.AddByRef(&a, &b);
        Console.Write("{0} {1} ", a.X, a.Y);
    }
}
