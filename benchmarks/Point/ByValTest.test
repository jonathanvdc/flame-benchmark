
build
{
    $(flame-benchmark) bin/$(FileName).flo -platform clr $(Flags) --iterations=$(Iterations) --harness=$(Harness) -o bin/$(FileName).exe;
}

run
{
    $(net-runtime) bin/$(FileName).exe;
}

configs
{
    Og { Flags = -Og; }
    O3 { Flags = $(OptFlags); }
}

init
{
    FileName = ByValTest;
    Iterations = 1;
    $dsc PointDef.ds $(FileName).ds -platform ir -runtime clr -o bin/$(FileName).flo -Wno-build;
}
